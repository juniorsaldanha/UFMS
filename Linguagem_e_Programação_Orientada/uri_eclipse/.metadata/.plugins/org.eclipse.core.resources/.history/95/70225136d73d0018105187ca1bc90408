public class ex12{
	public static void main(String[] args) {
		for(int i = 0; i < 3; ++i){
			for (int j = 0; j < 3; ++j){
				if(i % j == 0){
					throw new ArithmeticException();
					continue;
				}else{
					System.out.println("i = "+i + "		j = " +j);
				}
			}
		}
	}
}