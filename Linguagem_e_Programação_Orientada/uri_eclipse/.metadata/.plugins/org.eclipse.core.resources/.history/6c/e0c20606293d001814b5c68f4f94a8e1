import java.util.Scanner;
import ex09_convert.java;
public class ex09 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		convert w = new convert();
		w.fah = sc.nextDouble();
		w.cel = function__(w.fah);
		System.out.println("Celsius = " + w.cel);
		sc.close();
	}
	
}