import java.util.Scanner;
public class ex05 {
	public static void main(final String[] args) {
		final Scanner sc = new Scanner(System.in);
		final String v = sc.next();
		int count = 0;
		for(int i = 0; i < v.length(); i++)
			if(v.charAt(i) == '1') ++count;
		System.out.println(count);
		sc.close();
	}
}
